import Foundation
import RxSwift

final class Template<Element> {
    
    let observable: Observable<Element>
    
    init(_ subscribe: @escaping (AnyObserver<Element>) -> Disposable) {
        observable = Observable.create(subscribe)
            .subscribeOn(MainScheduler.instance)
    }
}
